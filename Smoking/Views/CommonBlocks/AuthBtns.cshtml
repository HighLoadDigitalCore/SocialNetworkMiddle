@if (!AccessHelper.IsAuth)
{
    <div class="btn_login">
        <a data-fancybox="#enter" class="btn -blue -huge -shaded" href="">Войти</a>
    </div>
    @Html.Action("AuthPopup")
    <div class="social-login">
        <h6>Войти через соцсеть:</h6>
        <div class="cols-wrapper">
            @Html.Action("Socials", "SocialAuth")
        </div>
    </div>
}